{
  "name": "nestjs-pubsub-transport",
  "version": "0.0.0",
  "description": "Custom transport for Google PubSub for NestJS framework",
  "keywords": [
    "pubsub",
    "google",
    "transport",
    "server",
    "nest",
    "nestjs"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Max Dovhopolyi <dovgopolyy0000@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdovhopo/nestjs-pubsub-transport"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "build:docs": "typedoc --out docs index.ts",
    "test": "jest --config jest.config.ts --coverage",
    "test:watch": "jest --config jest.config.ts --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@google-cloud/pubsub": "3.0.0",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/microservices": "8.4.5",
    "nest-winston": "1.6.2",
    "reflect-metadata": "0.1.13",
    "winston": "3.7.2"
  },
  "dependencies": {
    "error-to-json": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@google-cloud/pubsub": "3.0.0",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/microservices": "8.4.5",
    "@nestjs/testing": "^8.2.3",
    "@types/jest": "27.0.3",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "27.3.1",
    "jest-config": "27.3.1",
    "lint-staged": "^12.1.2",
    "nest-winston": "1.6.2",
    "prettier": "^2.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2",
    "winston": "3.7.2"
  }
}
